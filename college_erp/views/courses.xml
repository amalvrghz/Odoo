<odoo>
    <record id="course_tree" model="ir.ui.view">
        <field name="name">Course</field>
        <field name="model">college.course</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="category"/>
                <field name="duration"/>
                <field name="num_of_sem"/>
            </tree>
        </field>
    </record>
     <record id="course_form" model="ir.ui.view">
            <field name="name">Course</field>
            <field name="model">college.course</field>
            <field name="arch" type="xml">
                <form>
                        <sheet>
                                <group>
                                    <field name="name"/>
                                    <field name="category"/>
                                    <field name="duration"/>
                                    <field name="num_of_sem"/>
                                </group>
                            <notebook>
                                <page string="Semesters">
                                    <field name="semesters_ids"/>
                                </page>
                            </notebook>
                        </sheet>
                </form>
            </field>
        </record>
 </odoo>